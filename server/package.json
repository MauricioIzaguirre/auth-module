{
  "name": "auth-module-2025-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Generic authentication module with TypeScript, Express 5, PostgreSQL, and RBAC system",
  "main": "index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:migrate": "tsx src/database/migrations/index.ts",
    "db:seed": "tsx src/seeds/index.ts",
    "db:reset": "tsx src/database/reset.ts",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean"
  },
  "keywords": [
    "authentication",
    "typescript",
    "express",
    "postgresql",
    "rbac",
    "jwt",
    "bcrypt",
    "nodejs"
  ],
  "author": "Mauricio Izaguirre Narnajo",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  }
}
