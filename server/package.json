{
  "name": "auth-module-2025",
  "version": "1.0.0",
  "type": "module",
  "description": "Generic authentication module with TypeScript, Express 5, PostgreSQL, and RBAC system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:migrate": "tsx src/database/migrations/index.ts",
    "db:seed": "tsx src/seeds/index.ts",
    "db:reset": "tsx src/database/reset.ts"
  },
  "keywords": [
    "authentication",
    "typescript",
    "express",
    "postgresql",
    "rbac",
    "jwt",
    "bcrypt",
    "nodejs"
  ],
  "author": "Mauricio Izaguirre Narnajo",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "pg": "^8.13.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.1",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.18.0",
    "@types/pg": "^8.11.10",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/cookie-parser": "^1.4.7",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "tsc-alias": "^1.8.10",
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}